{
  "name": "booru-uploader",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "pnpm build-lib && wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "build-lib": "wasm-pack build lib/imagehash --target web",
    "sign": "tsx utils/signExtension.ts"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@tanstack/vue-query": "^5.83.1",
    "@vueuse/core": "^13.6.0",
    "file64": "^1.0.5",
    "idb": "^8.0.3",
    "imagehash-rs": "link:lib/imagehash/pkg",
    "ky": "^1.8.2",
    "mdi-vue": "^3.0.13",
    "tailwindcss": "^4.1.12",
    "uuidv7": "^1.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/shelljs": "^0.8.17",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/module-vue": "^1.0.2",
    "clipanion": "4.0.0-rc.4",
    "cross-var": "^1.1.0",
    "dotenv": "^17.2.1",
    "jwt-simple": "^0.5.6",
    "shelljs": "^0.10.0",
    "typescript": "^5.9.2",
    "vue-tsc": "^2.2.10",
    "wxt": "^0.20.8",
    "yocto-spinner": "^1.0.0"
  }
