    "uuidv7": "^1.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/module-vue": "^1.0.2",
    "amo-upload": "^0.6.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^9.0.0",
    "pyodide": "^0.28.0",
    "sass-embedded": "^1.89.2",
    "typescript": "5.6.3",
    "vite": "^7.0.6",
